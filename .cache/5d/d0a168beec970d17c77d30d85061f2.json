eStyle.scss":"@media only screen and (max-width: 820px) {\r\n  $text: #ddd;\r\n  $bg: linear-gradient(\r\n    115deg,\r\n    rgba(179, 27, 152, 1) 0%,\r\n    rgba(131, 114, 186, 1) 49%,\r\n    rgba(0, 212, 255, 1) 100%\r\n  );\r\n  * {\r\n    margin: 0;\r\n    padding: 0;\r\n    font-family: SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\",\r\n      \"Courier New\", monospace;\r\n  }\r\n  html {\r\n    overflow-x: hidden;\r\n    scroll-behavior: smooth;\r\n    color: $text;\r\n  }\r\n  img {\r\n    display: none;\r\n  }\r\n  .section1 {\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background: $bg;\r\n    .nav1 {\r\n      width: 100%;\r\n      height: 10%;\r\n      float: left;\r\n      .hamburger--menu {\r\n        width: 3rem;\r\n        height: 3rem;\r\n        top: 1rem;\r\n        left: 1.5rem;\r\n        position: absolute;\r\n        position: fixed;\r\n        background-color: transparent;\r\n        cursor: pointer;\r\n        z-index: 9;\r\n        @mixin bar {\r\n          width: 100%;\r\n          height: 10%;\r\n          margin-top: 20%;\r\n          background-color: $text;\r\n          border-radius: 5px;\r\n        }\r\n        .bar1 {\r\n          @include bar();\r\n        }\r\n        .bar2 {\r\n          @include bar();\r\n        }\r\n        .bar3 {\r\n          @include bar();\r\n        }\r\n      }\r\n    }\r\n    .nav2 {\r\n      width: 80%;\r\n      display: flex;\r\n      float: left;\r\n      justify-content: right;\r\n      align-items: center;\r\n      display: none;\r\n    }\r\n    ul li {\r\n      float: left;\r\n      list-style-type: none;\r\n      margin-right: 6rem;\r\n      font-size: 2vw;\r\n      margin-top: 2%;\r\n      cursor: pointer;\r\n      transition: all 0.3s linear;\r\n      border-radius: 2px;\r\n      width: 8vw;\r\n      text-align: center;\r\n      display: none;\r\n      &:hover {\r\n        background-color: white;\r\n        color: rgba(131, 114, 186, 1);\r\n        box-shadow: 0px 0px 5px 5px #ddd;\r\n      }\r\n    }\r\n    .menu {\r\n      width: 100%;\r\n      background-color: rgb(37, 29, 29);\r\n      height: 100%;\r\n      position: absolute;\r\n      left: 0;\r\n      top: 0;\r\n      position: fixed;\r\n      z-index: 8;\r\n      overflow: scroll;\r\n      &::-webkit-scrollbar {\r\n        display: none;\r\n      }\r\n      ul {\r\n        width: 100%;\r\n        height: 100%;\r\n\r\n        li {\r\n          text-align: center;\r\n          margin-left: 2.5%;\r\n          margin-right: 2.5%;\r\n          border-bottom: 2px solid white;\r\n          width: 95%;\r\n          margin-top: 25%;\r\n          transform: translate(-25rem, 0);\r\n          display: block;\r\n        }\r\n      }\r\n    }\r\n    .main {\r\n      width: 100%;\r\n      height: 90%;\r\n      display: flex;\r\n\r\n      .leftSide {\r\n        width: 100vw;\r\n        height: auto;\r\n        float: left;\r\n        display: block;\r\n\r\n        .gettingStarted {\r\n          width: 10rem;\r\n          height: 3rem;\r\n          outline: 2px solid white;\r\n          border-radius: 1;\r\n          appearance: button;\r\n          color: $text;\r\n          font-size: 1rem;\r\n          font-weight: bold;\r\n          transition: all 0.3s linear;\r\n\r\n          &:hover {\r\n            background-color: white;\r\n            color: rgba(131, 114, 186, 1);\r\n            cursor: pointer;\r\n            box-shadow: 0px 0px 5px 5px #ddd;\r\n          }\r\n        }\r\n      }\r\n      .rightSide {\r\n        width: 100%;\r\n        float: left;\r\n        display: block;\r\n        p {\r\n          font-size: 2.5rem;\r\n          width: 100%;\r\n          &:first-child {\r\n            top: 20.4rem;\r\n            text-decoration: underline;\r\n          }\r\n          &:nth-child(2) {\r\n            text-decoration: overline;\r\n          }\r\n          &:last-child {\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .section2 {\r\n    color: $text;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background: linear-gradient(\r\n      425deg,\r\n      rgba(179, 27, 152, 1) 0%,\r\n      rgba(131, 114, 186, 1) 49%,\r\n      rgba(0, 212, 255, 1) 100%\r\n    );\r\n    .articles {\r\n      color: $text;\r\n      width: 100vw;\r\n      height: 100vh;\r\n      background-color: transparent;\r\n      article {\r\n        width: 50%;\r\n        height: 100%;\r\n        float: left;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        p {\r\n          width: 50%;\r\n          font-size: 1.2vw;\r\n          &::first-letter {\r\n            font-size: 3vw;\r\n          }\r\n        }\r\n      }\r\n      aside {\r\n        width: 50%;\r\n        height: 100%;\r\n        float: left;\r\n        font-size: 1vw;\r\n        .img--container1 {\r\n          width: 100%;\r\n          height: 33%;\r\n          display: flex;\r\n          justify-content: right;\r\n          align-items: center;\r\n          font-size: 1vw;\r\n          article {\r\n            transform: translate(-10rem);\r\n          }\r\n          .pug {\r\n            transform: scale(1.1rem);\r\n            transform: translate(-5rem);\r\n          }\r\n        }\r\n        .img--container2 {\r\n          width: 100%;\r\n          height: 33%;\r\n          display: flex;\r\n          font-size: 1vw;\r\n\r\n          .gsap {\r\n            float: right;\r\n            transform: scale(2rem);\r\n          }\r\n        }\r\n        .img--container3 {\r\n          width: 100%;\r\n          height: 33%;\r\n          display: flex;\r\n          justify-content: right;\r\n          align-items: right;\r\n          font-size: 1vw;\r\n          .sass {\r\n            transform: scale(0.5);\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"06e16f12cdfaaa49df688f61045811e8","cacheData":{"env":{}}}